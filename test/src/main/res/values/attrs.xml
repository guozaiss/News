<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WavyLineView">
        <attr name="period" format="float"/>
        <attr name="strokeColor" format="color"/>
        <attr name="amplitude" format="dimension"/>
        <attr name="strokeWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="DeSwitchGroup">
        <attr name="orientation1">
            <flag name="horizontal" value="0"></flag>
            <flag name="vertical" value="1"></flag>
        </attr>
        <attr name="pinned_view1" format="reference" />
        <attr name="pinned_item1" format="reference" />
        <attr name="pinned_item_array1" format="reference" />
    </declare-styleable>
</resources>